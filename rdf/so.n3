# Grouping of events

:Bar
	a owl:Class;
	rdfs:subClassOf event:Event;
	rdfs:label "bar";
	.

:Tied
	a owl:Class;
	rdfs:subClassOf event:Event;
	rdfs:label "tied notes";
	rdfs:label "slurred notes (different pitches (articulation))"; # Should be two different things - cannam
	rdfs:label "notes liees (same pitch, adjacent notes)";
	.

:Motif
	a owl:Class;
	rdfs:subClassOf event:Event;
	rdfs:label "motif";
	.

:Octave
	a owl:Class;
	# Not sure what it should be
	.

:Timbre
	a owl:Class;
	# Not sure what it should be 
	.

:Accidental
	a owl:Class;
	.

# Properties



:pitch 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:subPropertyOf event:factor;
	rdfs:label "pitch";
	rdfs:domain :NoteEvent;
	rdfs:range :Pitch;
	.

:timbre
	a owl:ObjectProperty;
	rdfs:subPropertyOf event:factor;
	rdfs:label "timbre";
	rdfs:domain :NoteEvent;
	rdfs:range :Timbre;
	.

:note 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:label "note";
	rdfs:domain :Pitch;
	rdfs:range :Note;
	.

:accidental 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:label "accidental";
	rdfs:domain :Note;
	rdfs:range :Accidental;
	.

:octave
	a owl:ObjectProperty;
	rdfs:label "octave";
	rdfs:domain :Note;
	rdfs:range :Octave;
	.


# Individuals

:a a :Note.
:b a :Note.
:c a :Note.
:d a :Note.
:e a :Note.
:f a :Note.
:g a :Note.

:sharp a :Accidental.
:natural a :Accidental.
:flat a :Accidental.
:double_sharp a :Accidental.
:double_flat a :Accidental.
:double_natural a :Accidental. # This is just a joke :-)


