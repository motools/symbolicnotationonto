# Properties



:pitch 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:subPropertyOf event:factor;
	rdfs:label "pitch"@en;
	rdfs:domain :NoteEvent;
	rdfs:range :Pitch;
	.

:timbre
	a owl:ObjectProperty;
	rdfs:subPropertyOf event:factor;
	rdfs:label "timbre"@en;
	rdfs:domain :NoteEvent;
	rdfs:range :Timbre;
	.

:note 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:label "note"@en;
	rdfs:domain :Pitch;
	rdfs:range :Note;
	.

:accidental 
	a owl:ObjectProperty;
	a owl:FunctionalProperty;
	rdfs:label "accidental"@en;
	rdfs:domain :Note;
	rdfs:range :Accidental;
	.

:octave
	a owl:ObjectProperty;
	rdfs:label "octave"@en;
	rdfs:domain :Note;
	rdfs:range :Octave;
	.


